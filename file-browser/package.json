{
  "name": "@domino/file-browser",
  "version": "0.0.0",
  "main": "lib/index.js",
  "reactComponentsMain": "src/index.js",
  "license": "UNLICENSED",
  "scripts": {
    "build": "domino-build-utils build",
    "watch": "domino-build-utils watch",
    "lint": "domino-build-utils lint",
    "format": "domino-build-utils format",
    "test": "jest",
    "precommit": "npm run lint && npm run format",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/mocks/fileMock.js",
      "\\.(css|less)$": "<rootDir>/tests/mocks/styleMock.js"
    }
  },
  "dependencies": {
    "@domino/build-utils": "*",
    "@domino/shared-components": "*",
    "@domino/style": "*",
    "@manaflair/redux-batch": "^0.1.0",
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "date-fns": "^1.28.4",
    "dom-helpers": "^3.2.1",
    "enzyme": "^2.9.1",
    "jquery": "^3.2.1",
    "js-search": "^1.4.2",
    "memoize-weak": "^1.0.1",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.1",
    "react-dom": "^15.6.1",
    "react-infinite": "^0.11.0",
    "react-overlays": "^0.7.0",
    "react-redux": "^5.0.6",
    "recompose": "^0.25.0",
    "redux": "^3.7.2",
    "styled-components": "^2.0.0",
    "uncontrollable": "^4.1.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.1",
    "jest": "^20.0.1",
    "lorem-ipsum": "^1.0.4",
    "random-seed": "^0.3.0",
    "react-dom": "^15.6.1",
    "storyshots": "^3.2.2"
  }
}
